<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wrc195.wrc195task.DirectoryView">
   <children>
      <VBox alignment="CENTER" layoutX="100.0" prefHeight="400.0" prefWidth="300.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onActionAppointments" prefHeight="100.0" prefWidth="200.0" text="Appointments">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
            <Button layoutX="60.0" layoutY="160.0" mnemonicParsing="false" onAction="#onActionCustomers" prefHeight="100.0" prefWidth="200.0" text="Customers">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
            <Button layoutX="60.0" layoutY="210.0" mnemonicParsing="false" onAction="#onActionLogOut" prefHeight="75.0" prefWidth="175.0" text="Log Out" />
         </children>
      </VBox>
   </children>
</AnchorPane>
